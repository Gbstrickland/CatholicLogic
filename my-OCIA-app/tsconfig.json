{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node16",
    "customConditions": ["react-native"],
    "target": "esnext",
    "module": "node16",
    "jsx": "react-jsx",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
